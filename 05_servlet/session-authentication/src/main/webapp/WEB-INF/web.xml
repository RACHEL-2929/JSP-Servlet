  </servlet>
  <servlet-mapping>
    <servlet-name>MemberLoginServlet</servlet-name>
    <url-pattern>/MemberLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MemberLogoutServlet</display-name>
    <servlet-name>MemberLogoutServlet</servlet-name>
